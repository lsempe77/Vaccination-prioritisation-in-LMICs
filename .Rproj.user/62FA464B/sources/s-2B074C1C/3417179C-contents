r1 <- nimue::run(
  country = "India",
  R0 = 1.5, 
  seed = 1,
   dur_V = 365/.8,
  dur_R = 365/.8)

x <- nimue::format(r1,
                   compartments = NULL,
                   summaries = c("deaths", "infections", "vaccines", "hospitalisations"),
                   reduce_age = FALSE) %>%
  mutate(t = t + t_start)
